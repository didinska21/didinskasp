# ============================================
# ASP BOT CONFIGURATION
# ============================================

# ============================================
# REFERRAL CODE (WAJIB DIISI!)
# ============================================
# Masukkan kode referral ASP kamu di sini
# Contoh: _qnMfT1bT8DWgatXG3r7_96_lMe_G89I6L78zAwEu9OicHu8yI88DLolatokRlMDynFqXA==
REFERRAL_CODE=

# ============================================
# JUMLAH AKUN YANG INGIN DIBUAT
# ============================================
# Berapa banyak akun yang akan di-generate
# Nilai: Angka positif (1, 5, 10, 100, dst)
ACCOUNTS_TO_CREATE=5

# ============================================
# PROXY SETTINGS
# ============================================

# Gunakan Proxy atau Tidak?
# true  = Pakai proxy dari file proxy.txt
# false = Tidak pakai proxy (langsung dari IP kamu)
USE_PROXY=false

# Mode Proxy (hanya berlaku jika USE_PROXY=true)
# Rotating = Ganti proxy setiap akun (akun 1 pakai proxy 1, akun 2 pakai proxy 2, dst)
#            Bagus untuk bypass rate limit & avoid detection
#            
# Static   = Random 1 proxy dari list, semua akun pakai proxy yang sama
#            Lebih cepat tapi risiko kena ban lebih tinggi
PROXY_MODE=Rotating

# ============================================
# EMAIL SETTINGS
# ============================================

# Provider Email Temporary
# random        = Random antara mail.tm dan guerrillamail (Recommended)
# mail.tm       = Hanya gunakan mail.tm
# guerrillamail = Hanya gunakan guerrillamail
EMAIL_PROVIDER=random

# Maksimal Percobaan Cek Inbox
# Berapa kali bot akan cek inbox untuk kode verifikasi
# Nilai: 10-20 (Recommended: 15)
MAX_EMAIL_CHECK_ATTEMPTS=15

# Delay Antar Pengecekan Email (milliseconds)
# Waktu tunggu antara setiap cek inbox
# Nilai: 5000-10000 ms (Recommended: 8000 = 8 detik)
EMAIL_CHECK_DELAY=8000

# ============================================
# DELAY SETTINGS
# ============================================

# Delay Minimum Antar Akun (milliseconds)
# Waktu tunggu minimum sebelum buat akun berikutnya
# Nilai: 20000-30000 ms (Recommended: 25000 = 25 detik)
DELAY_MIN=25000

# Delay Maximum Antar Akun (milliseconds)
# Waktu tunggu maksimum sebelum buat akun berikutnya
# Nilai: 35000-50000 ms (Recommended: 40000 = 40 detik)
DELAY_MAX=40000

# ============================================
# OUTPUT SETTINGS
# ============================================

# File Output untuk Menyimpan Akun
# Nama file tempat menyimpan hasil registrasi
OUTPUT_FILE=registered_accounts.json

# File Input untuk Proxy List
# Nama file yang berisi daftar proxy (1 proxy per baris)
PROXY_FILE=proxy.txt

# ============================================
# ADVANCED SETTINGS (Opsional - Jarang Diubah)
# ============================================

# Show Debug Logs
# true  = Tampilkan log detail untuk debugging
# false = Hanya tampilkan log penting
DEBUG=false

# Auto Retry on Failure
# true  = Otomatis retry jika gagal
# false = Skip ke akun berikutnya jika gagal
AUTO_RETRY=false

# Max Retry Attempts (hanya berlaku jika AUTO_RETRY=true)
MAX_RETRIES=3
